<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.control">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DashControl"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.com.marcoa.control.Contato"></activity>
        <activity android:name="com.example.com.marcoa.control.Usuarios" />
        <activity android:name="com.example.com.marcoa.control.VerProgramacoesWeb" />
        <activity android:name="com.example.com.marcoa.control.Teste" />
        <activity android:name="com.example.com.marcoa.control.PersonalizarIcones" />
        <activity android:name="com.example.com.marcoa.control.NovaProgramacao" />
        <activity android:name="com.example.com.marcoa.control.DataEspWeb" />
        <activity
            android:name="com.example.com.marcoa.control.DashWeb"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.com.marcoa.control.CadastrarUsuario"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.com.marcoa.control.SetDataEsp"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.com.marcoa.control.DashLocal"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.com.marcoa.control.Login"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.DashControl.NoActionBar" />
        <activity
            android:name="com.example.com.marcoa.control.ConfigureEsp"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.com.marcoa.control.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>